{
  "name": "@healerlab/device-js",
  "private": false,
  "version": "0.0.4",
  "description": "Powerful, lightweight device detector module",
  "main": "./dist/h-device.umd.cjs",
  "module": "./dist/h-device.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/h-device.umd.cjs",
      "require": "./dist/h-device.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "device detector",
    "user agent",
    "cloudflare",
    "navigator"
  ],
  "author": "HealerNguyen",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.5.3",
    "vitepress": "^1.0.0-rc.10",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "license": "MIT",
  "homepage": "https://healerlab.github.io/device-js",
  "bugs": {
    "url": "https://github.com/healerlab/device-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/healerlab/device-js.git"
  },
  "dependencies": {
    "@healerlab/device-js": "^0.0.4"
